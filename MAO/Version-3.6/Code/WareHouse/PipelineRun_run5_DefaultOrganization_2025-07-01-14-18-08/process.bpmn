      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Confirm stroke unit requirements: co-located beds occupied by stroke patients â‰¥75% of the time, dedicated interprofessional team with expertise (nursing, physiotherapy, occupational therapy, speech-language pathology, medicine, social work, dietitian), team members spend majority of time treating stroke patients, regular stroke education, and adequate staffing 7 days per week (Role: hospital administration; Objects: stroke unit beds, interprofessional team, staff, education records, staffing schedule)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Admit patient to specialized, geographically defined stroke unit (Role: admitting physician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity5_modified" name="Within 48 hours of admission, perform comprehensive assessment using standardized, valid tools to evaluate stroke-related impairments and functional status, including AlphaFIM, venous thromboembolism risk, and nutrition/hydration status, and formulate a management plan based on all assessment findings (Role: physiotherapist, occupational therapist, speech-language pathologist, clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: patient)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity9" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: patient)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity11_modified" name="If nutritional concerns, hydration deficits, dysphagia, or other comorbidities are identified, refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support; otherwise, continue routine monitoring (Role: clinical team; Objects: patient, referral)">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity13" name="Document all assessments and interventions using standardized, validated tools in electronic format linked to EHR (Role: clinical team; Objects: EHR, assessment tools)">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:task>
