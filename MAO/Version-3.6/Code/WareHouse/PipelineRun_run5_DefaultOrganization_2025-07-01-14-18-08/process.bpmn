      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Determine eligibility for acute inpatient admission per recommendation 2.4.2 (Role: admitting physician; Objects: patient, eligibility criteria)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_22</bpmn:outgoing>
    </bpmn:task>
