      <ns0:outgoing>Flow_1</ns0:outgoing>
    </ns0:startEvent>
    <ns0:endEvent id="EndEvent_1" name="End">
      <ns0:incoming>Flow_28</ns0:incoming>
    </ns0:endEvent>
    <ns0:task id="activity1-1" name="Verify patient eligibility for admission to stroke unit (Role: hospital admission staff; Objects: patient, eligibility criteria)">
      <ns0:incoming>Flow_1</ns0:incoming>
      <ns0:outgoing>Flow_2</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity1-2" name="Assign patient to available co-located stroke unit bed (Role: hospital admission staff; Objects: patient, stroke unit beds)">
      <ns0:incoming>Flow_2</ns0:incoming>
      <ns0:outgoing>Flow_3</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity2" name="Ensure core interprofessional stroke team is present (medicine, nursing, OT, PT, SLP, social work, dietitian) (Role: hospital administration; Objects: staff roster, stroke unit)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Ensure stroke unit team has stroke expertise and ongoing education (Role: hospital administration; Objects: training records, staff)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4" name="Ensure interprofessional stroke team is available 7 days a week (Role: hospital administration; Objects: staff schedule)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-1" name="Conduct physiotherapy assessment within 48 hours of admission (Role: PT; Objects: patient, assessment tools)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_7</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-2" name="Conduct occupational therapy assessment within 48 hours of admission (Role: OT; Objects: patient, assessment tools)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_8</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-3" name="Conduct speech-language pathology assessment within 48 hours of admission (Role: SLP; Objects: patient, assessment tools)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_9</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-4" name="Formulate management plan based on assessments (Role: PT, OT, SLP; Objects: patient, assessment results)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7" name="Use standardized, valid assessment tools to evaluate stroke-related impairments and functional status (Role: clinicians; Objects: assessment tools, patient)">
      <ns0:incoming>Flow_10</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Complete AlphaFIM on or by day 3 after admission (admission day is day 1) (Role: clinicians; Objects: AlphaFIM, patient)">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity9" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinicians; Objects: patient, risk assessment tool)">
      <ns0:incoming>Flow_12</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity10" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: patient)">
      <ns0:incoming>Flow_13</ns0:incoming>
      <ns0:outgoing>Flow_14</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity11" name="Assess nutrition and hydration status within first 48 hours of admission using valid screening tool (Role: clinicians; Objects: patient, screening tool)">
      <ns0:incoming>Flow_14</ns0:incoming>
      <ns0:outgoing>Flow_19</ns0:outgoing>
    </ns0:task>
