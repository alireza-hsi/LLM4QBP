<?xml version='1.0' encoding='UTF-8'?>
<ns0:definitions xmlns:ns0="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:ns2="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:ns3="http://www.omg.org/spec/DD/20100524/DC" xmlns:ns4="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <ns0:process id="Process_1" name="Stroke Process" isExecutable="true">
    <ns0:startEvent id="StartEvent_1" name="Start">
      <ns0:outgoing>Flow_1</ns0:outgoing>
    </ns0:startEvent>
    <ns0:endEvent id="EndEvent_1" name="End">
      <ns0:incoming>Flow_20</ns0:incoming>
    </ns0:endEvent>
    <ns0:task id="activity1-1" name="Verify patient eligibility for admission to stroke unit (Role: hospital admission staff; Objects: patient, eligibility criteria)">
      <ns0:incoming>Flow_1</ns0:incoming>
      <ns0:outgoing>Flow_2</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity1-2" name="Assign patient to available bed in specialized, geographically defined stroke unit with co-located beds and dedicated interprofessional team (Role: hospital admission staff; Objects: patient, stroke unit)">
      <ns0:incoming>Flow_2</ns0:incoming>
      <ns0:outgoing>Flow_3</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity2" name="Ensure core stroke unit team includes medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) (Role: hospital administration; Objects: staff roster)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Ensure stroke unit staff spend majority of time treating stroke patients and complete regular stroke care education (Role: hospital administration; Objects: staff training records)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4" name="Ensure interprofessional stroke team is available 7 days a week (Role: hospital administration; Objects: staff schedule)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-1" name="Conduct initial assessment of stroke patient within 48 hours of admission (Role: physiotherapist, occupational therapist, speech-language pathologist; Objects: patient, assessment tools)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_7</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5-2" name="Formulate individualized management plan based on assessment findings (Role: physiotherapist, occupational therapist, speech-language pathologist; Objects: patient, assessment results)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_8</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7-1" name="Select appropriate standardized, valid assessment tools (Role: clinicians; Objects: assessment tools)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_9</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7-2" name="Administer selected assessment tools to patient (Role: clinicians; Objects: assessment tools, patient)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity9" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinicians; Objects: risk assessment tool)">
      <ns0:incoming>Flow_10</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity10" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: patient)">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity11" name="Assess nutrition and hydration status within first 48 hours of admission using valid screening tool (Role: clinicians; Objects: screening tool)">
      <ns0:incoming>Flow_12</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Complete AlphaFIM assessment on or by day 3 after admission (admission day is day 1) (Role: clinicians; Objects: AlphaFIM tool)">
      <ns0:incoming>Flow_13</ns0:incoming>
      <ns0:outgoing>Flow_18</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <ns0:incoming>Flow_18</ns0:incoming>
      <ns0:outgoing>Flow_14</ns0:outgoing>
      <ns0:outgoing>Flow_16</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity12" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet nutritional/fluid requirements (Role: clinicians; Objects: patient, referral form)">
      <ns0:incoming>Flow_14</ns0:incoming>
      <ns0:outgoing>Flow_15</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity13" name="Continue standard monitoring (Role: clinicians; Objects: patient)">
      <ns0:incoming>Flow_16</ns0:incoming>
      <ns0:outgoing>Flow_17</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1Join" name="Exclusive Merge">
      <ns0:incoming>Flow_15</ns0:incoming>
      <ns0:incoming>Flow_17</ns0:incoming>
      <ns0:outgoing>Flow_19</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity14" name="Provide cross-continuum secondary prevention assessments and therapies as per Modules 5 and 10 before discharge or transfer (Role: clinical team; Objects: patient, prevention protocols)">
      <ns0:incoming>Flow_19</ns0:incoming>
      <ns0:outgoing>Flow_20</ns0:outgoing>
    </ns0:task>
    <ns0:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="activity1-1" />
    <ns0:sequenceFlow id="Flow_2" sourceRef="activity1-1" targetRef="activity1-2" />
    <ns0:sequenceFlow id="Flow_3" sourceRef="activity1-2" targetRef="activity2" />
