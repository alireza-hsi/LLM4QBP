    <ns0:sequenceFlow id="Flow_13" sourceRef="activity11" targetRef="activity8" />
    <ns0:sequenceFlow id="Flow_14" name="Nutritional concerns, hydration deficits, dysphagia, or other comorbidities present" sourceRef="exclusivegateway1" targetRef="activity12">
      <ns0:conditionExpression xsi:type="bpmn:tFormalExpression">Nutritional concerns, hydration deficits, dysphagia, or other comorbidities present</ns0:conditionExpression>
    </ns0:sequenceFlow>
    <ns0:sequenceFlow id="Flow_15" sourceRef="activity12" targetRef="exclusivegateway1Join" />
    <ns0:sequenceFlow id="Flow_16" name="No nutritional concerns or deficits" sourceRef="exclusivegateway1" targetRef="activity13">
      <ns0:conditionExpression xsi:type="bpmn:tFormalExpression">No nutritional concerns or deficits</ns0:conditionExpression>
    </ns0:sequenceFlow>
    <ns0:sequenceFlow id="Flow_17" sourceRef="activity13" targetRef="exclusivegateway1Join" />
    <ns0:sequenceFlow id="Flow_18" sourceRef="activity8" targetRef="exclusivegateway1" />
    <ns0:sequenceFlow id="Flow_19" sourceRef="exclusivegateway1Join" targetRef="activity14" />
    <ns0:sequenceFlow id="Flow_20" sourceRef="activity14" targetRef="EndEvent_1" />
  </ns0:process>
