      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_29</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="activity1-1" name="Verify patient eligibility for admission to stroke unit (Role: hospital admission staff; Objects: patient, admission criteria)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity1-2" name="Assign patient to available co-located bed in stroke unit (Role: hospital admission staff; Objects: patient, stroke unit bed)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Ensure core stroke unit team includes medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) (Role: hospital administration; Objects: staff roster)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Ensure stroke unit team members spend majority of time treating stroke patients and complete regular stroke care education (Role: hospital administration; Objects: staff training records)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Ensure interprofessional stroke team is available 7 days a week (Role: hospital administration; Objects: staff schedule)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6-1" name="Evaluate patient for contraindications to early mobilization (Role: clinical staff; Objects: patient, clinical criteria)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5-1" name="Conduct initial assessment by physiotherapy, speech-language pathology, and occupational therapy team (Role: physiotherapy, speech-language pathology, occupational therapy team; Objects: patient, assessment tools)">
