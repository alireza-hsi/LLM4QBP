      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5-2" name="Formulate individualized management plan based on assessment findings and mobilization decision (Role: physiotherapy, speech-language pathology, occupational therapy team; Objects: patient, assessment results, mobilization status)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6-2" name="Mobilize patient between 24-48 hours of stroke onset if no contraindications (Role: clinical staff; Objects: patient)">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_27</bpmn:outgoing>
    </bpmn:task>
