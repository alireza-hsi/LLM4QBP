      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinical team; Objects: patient, risk assessment tool)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity9_new" name="Assess nutrition and hydration status within first 48 hours of admission using valid screening tool (Role: clinical team; Objects: patient, screening tool)">
