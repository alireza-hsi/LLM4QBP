      <ns0:outgoing>Flow_15</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8_new" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: patient, hydration protocol)">
