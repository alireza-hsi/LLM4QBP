      <ns0:outgoing>Flow_7</ns0:outgoing>
      <ns0:outgoing>Flow_8</ns0:outgoing>
    </ns0:parallelGateway>
    <ns0:task id="activity5" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: patient, mobilization protocol)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_9</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity6" name="Complete AlphaFIM assessment on or by day 3 after admission (admission day is day 1) (Role: clinical team; Objects: patient, AlphaFIM tool)">
