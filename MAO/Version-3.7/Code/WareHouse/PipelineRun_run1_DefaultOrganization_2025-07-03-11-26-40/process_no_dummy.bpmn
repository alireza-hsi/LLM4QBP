    </ns0:exclusiveGateway>
    <ns0:task id="activity2" name="Ensure stroke unit setup: dedicated interprofessional team (medicine, nursing, OT, PT, SLP, social work, dietitian), team members spend majority of time treating stroke patients, regular stroke education, and 7-day/week availability (Role: hospital administration; Objects: staffing roster, staffing schedule, education records)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: patient, stroke unit)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
