  <ns0:process id="Process_1" name="Stroke Process" isExecutable="true">
    <ns0:startEvent id="StartEvent_1" name="Start">
      <ns0:outgoing>Flow_1</ns0:outgoing>
    </ns0:startEvent>
    <ns0:endEvent id="EndEvent_1" name="End">
      <ns0:incoming>Flow_24</ns0:incoming>
    </ns0:endEvent>
    <ns0:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools)">
      <ns0:incoming>Flow_1</ns0:incoming>
      <ns0:outgoing>Flow_23</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <ns0:incoming>Flow_23</ns0:incoming>
      <ns0:outgoing>Flow_19</ns0:outgoing>
      <ns0:outgoing>Flow_21</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity2" name="Ensure stroke unit infrastructure and readiness: maintain geographically defined stroke unit with co-located beds, dedicated interprofessional team (nursing, physiotherapy, occupational therapy, speech-language pathology, medicine, social work, dietitian), 7-day staffing, and use of standardized validated assessment tools (preferably electronic, linked to EHR) (Role: hospital administration; Objects: stroke unit, staffing, assessment tools, EHR)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Assign patient to stroke unit bed (Role: admitting physician; Objects: patient, stroke unit bed)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4" name="Notify stroke unit team of new admission (Role: admitting physician; Objects: patient, notification system)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5" name="Maintain stroke expertise by dedicating majority of time to stroke care and completing regular stroke education (Role: stroke unit team; Objects: training records)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_7</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity13" name="Provide access to specialized interprofessional team 7 days a week (Role: stroke unit team; Objects: patient, team schedule)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_8</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity6" name="Perform comprehensive initial assessment within 48 hours using standardized, valid tools to evaluate stroke-related impairments and functional status, including screening for nutrition and hydration status, and immediately formulate individualized management plan (Role: physiotherapist, occupational therapist, speech-language pathologist, clinician; Objects: patient, assessment tools, care plan template, nutrition screening tool, hydration screening tool)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_9</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity18" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinician; Objects: patient, risk assessment tool)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: stroke unit team; Objects: patient, mobilization aids)">
      <ns0:incoming>Flow_10</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Complete AlphaFIM assessment on or by day 3 after admission (admission day is day 1) (Role: clinician; Objects: patient, AlphaFIM tool)">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity10" name="Encourage early mobilization and adequate hydration to help prevent venous thromboembolism (Role: stroke unit team; Objects: patient, hydration plan)">
      <ns0:incoming>Flow_12</ns0:incoming>
      <ns0:outgoing>Flow_17</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway">
      <ns0:incoming>Flow_17</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
      <ns0:outgoing>Flow_15</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity14" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements (Role: clinician; Objects: patient, referral form)">
      <ns0:incoming>Flow_13</ns0:incoming>
      <ns0:outgoing>Flow_14</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity15" name="Continue standard nutrition and hydration monitoring (Role: clinician; Objects: patient)">
      <ns0:incoming>Flow_15</ns0:incoming>
      <ns0:outgoing>Flow_16</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway2Join" name="Exclusive Merge">
      <ns0:incoming>Flow_14</ns0:incoming>
      <ns0:incoming>Flow_16</ns0:incoming>
      <ns0:outgoing>Flow_18</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity16" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (per Modules 5 and 10) regardless of discharge destination (Role: stroke unit team; Objects: patient, prevention protocols)">
      <ns0:incoming>Flow_18</ns0:incoming>
      <ns0:outgoing>Flow_20</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity17" name="Proceed with alternative care pathway (not covered in Module 3a) (Role: clinician; Objects: patient)">
      <ns0:incoming>Flow_21</ns0:incoming>
      <ns0:outgoing>Flow_22</ns0:outgoing>
    </ns0:task>
