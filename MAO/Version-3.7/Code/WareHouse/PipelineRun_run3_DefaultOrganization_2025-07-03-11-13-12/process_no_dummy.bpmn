    <ns0:sequenceFlow id="Flow_20" sourceRef="activity16" targetRef="exclusivegateway1Join" />
    <ns0:sequenceFlow id="Flow_21" name="Does not meet criteria for acute inpatient admission (e.g., TIA or minor/nondisabling stroke)" sourceRef="exclusivegateway1" targetRef="activity17">
      <ns0:conditionExpression xsi:type="bpmn:tFormalExpression">Does not meet criteria for acute inpatient admission (e.g., TIA or minor/nondisabling stroke)</ns0:conditionExpression>
    </ns0:sequenceFlow>
