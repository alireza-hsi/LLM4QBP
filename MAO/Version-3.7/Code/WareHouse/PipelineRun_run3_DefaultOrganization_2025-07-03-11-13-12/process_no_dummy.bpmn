    <ns0:sequenceFlow id="Flow_23" sourceRef="activity1" targetRef="exclusivegateway1" />
    <ns0:sequenceFlow id="Flow_24" sourceRef="exclusivegateway1Join" targetRef="EndEvent_1" />
