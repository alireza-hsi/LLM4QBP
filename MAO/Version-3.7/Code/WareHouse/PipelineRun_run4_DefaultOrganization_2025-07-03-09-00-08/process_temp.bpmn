<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn"><bpmn:process id="Process_1" isExecutable="true" name="Stroke Process"><bpmn:startEvent id="StartEvent_1" name="Start"><bpmn:outgoing>Flow_1</bpmn:outgoing></bpmn:startEvent><bpmn:endEvent id="EndEvent_1" name="End"><bpmn:incoming>Flow_26</bpmn:incoming></bpmn:endEvent><bpmn:task id="activity10" name="Ensure interprofessional stroke team uses standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: hospital administration; Objects: assessment tools, EHR)"><bpmn:incoming>Flow_1</bpmn:incoming><bpmn:outgoing>Flow_2</bpmn:outgoing></bpmn:task><bpmn:task id="activity11" name="Ensure adequate staffing 7 days per week for stroke/TIA care (Role: hospital administration; Objects: staffing plan)"><bpmn:incoming>Flow_2</bpmn:incoming><bpmn:outgoing>Flow_3</bpmn:outgoing></bpmn:task><bpmn:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools)"><bpmn:incoming>Flow_3</bpmn:incoming><bpmn:outgoing>Flow_25</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"><bpmn:outgoing>Flow_21</bpmn:outgoing><bpmn:outgoing>Flow_23</bpmn:outgoing><bpmn:incoming>Flow_25</bpmn:incoming></bpmn:exclusiveGateway><bpmn:task id="dummy_Flow_4" name="(dummy branch entry)"><bpmn:outgoing>Flow_5</bpmn:outgoing><bpmn:incoming>Flow_21</bpmn:incoming></bpmn:task><bpmn:task id="activity2_modified" name="Ensure stroke unit is established as a specialized, geographically defined unit with co-located beds (75% stroke occupancy), staffed by a dedicated interprofessional team (medicine, nursing, OT, PT, SLP, social work, dietitian) who spend the majority of their time treating stroke patients, regularly complete stroke education, and provide access 7 days a week (Role: hospital administration; Objects: stroke unit, staffing plan, education records, staffing schedule)"><bpmn:incoming>Flow_5</bpmn:incoming><bpmn:outgoing>Flow_6</bpmn:outgoing></bpmn:task><bpmn:task id="activity3_modified" name="Admit patient to specialized, geographically defined stroke unit (Role: admitting clinician; Objects: patient, stroke unit)"><bpmn:incoming>Flow_6</bpmn:incoming><bpmn:outgoing>Flow_11</bpmn:outgoing></bpmn:task><bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway"><bpmn:outgoing>Flow_7</bpmn:outgoing><bpmn:outgoing>Flow_8</bpmn:outgoing><bpmn:incoming>Flow_11</bpmn:incoming></bpmn:parallelGateway><bpmn:task id="activity4_modified" name="Select and use appropriate standardized, valid assessment tools; assess patient within 48 hours of admission for stroke-related impairments, functional status, AlphaFIM (by day 3), venous thromboembolism risk, nutrition and hydration status; and formulate a comprehensive management plan based on all assessment results (Role: PT, OT, SLP, clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results)"><bpmn:incoming>Flow_7</bpmn:incoming><bpmn:outgoing>Flow_9</bpmn:outgoing></bpmn:task><bpmn:task id="activity5_combined" name="Mobilize patient early (between 24-48 hours of stroke onset) and encourage adequate hydration to help prevent venous thromboembolism, unless contraindicated; avoid frequent out-of-bed activity within first 24 hours unless clinically justified (Role: clinical team; Objects: patient, mobilization protocol, hydration protocol)"><bpmn:incoming>Flow_8</bpmn:incoming><bpmn:outgoing>Flow_10</bpmn:outgoing></bpmn:task><bpmn:parallelGateway id="parallelgateway1Join" name="Parallel Merge"><bpmn:incoming>Flow_9</bpmn:incoming><bpmn:incoming>Flow_10</bpmn:incoming><bpmn:outgoing>Flow_16</bpmn:outgoing></bpmn:parallelGateway><bpmn:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"><bpmn:outgoing>Flow_12</bpmn:outgoing><bpmn:outgoing>Flow_14</bpmn:outgoing><bpmn:incoming>Flow_16</bpmn:incoming></bpmn:exclusiveGateway><bpmn:task id="activity6_modified" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements (Role: clinical team; Objects: patient, referral form)"><bpmn:incoming>Flow_12</bpmn:incoming><bpmn:outgoing>Flow_13</bpmn:outgoing></bpmn:task><bpmn:task id="activity7_modified" name="Continue standard nutrition and hydration monitoring (Role: clinical team; Objects: patient)"><bpmn:incoming>Flow_14</bpmn:incoming><bpmn:outgoing>Flow_15</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway2Join" name="Exclusive Merge"><bpmn:incoming>Flow_13</bpmn:incoming><bpmn:incoming>Flow_15</bpmn:incoming><bpmn:outgoing>Flow_17</bpmn:outgoing></bpmn:exclusiveGateway><bpmn:task id="activity9_modified_branch1" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (per Modules 5 and 10), regardless of discharge or in-hospital rehabilitation status (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_17</bpmn:incoming><bpmn:outgoing>Flow_22</bpmn:outgoing></bpmn:task><bpmn:task id="dummy_Flow_18" name="(dummy branch entry)"><bpmn:outgoing>Flow_19</bpmn:outgoing><bpmn:incoming>Flow_23</bpmn:incoming></bpmn:task><bpmn:task id="activity8_modified" name="Do not apply acute inpatient stroke unit recommendations; proceed with alternative care pathway (Role: clinical team; Objects: patient)"><bpmn:incoming>Flow_19</bpmn:incoming><bpmn:outgoing>Flow_20</bpmn:outgoing></bpmn:task><bpmn:task id="activity9_modified_branch2" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (per Modules 5 and 10), regardless of discharge or in-hospital rehabilitation status (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_20</bpmn:incoming><bpmn:outgoing>Flow_24</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway1Join" name="Exclusive Merge"><bpmn:incoming>Flow_22</bpmn:incoming><bpmn:incoming>Flow_24</bpmn:incoming><bpmn:outgoing>Flow_26</bpmn:outgoing></bpmn:exclusiveGateway><bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="activity10" /><bpmn:sequenceFlow id="Flow_2" sourceRef="activity10" targetRef="activity11" /><bpmn:sequenceFlow id="Flow_3" sourceRef="activity11" targetRef="activity1" /><bpmn:sequenceFlow id="Flow_5" sourceRef="dummy_Flow_4" targetRef="activity2_modified" /><bpmn:sequenceFlow id="Flow_6" sourceRef="activity2_modified" targetRef="activity3_modified" /><bpmn:sequenceFlow id="Flow_7" sourceRef="parallelgateway1" targetRef="activity4_modified" /><bpmn:sequenceFlow id="Flow_8" sourceRef="parallelgateway1" targetRef="activity5_combined" /><bpmn:sequenceFlow id="Flow_9" sourceRef="activity4_modified" targetRef="parallelgateway1Join" /><bpmn:sequenceFlow id="Flow_10" sourceRef="activity5_combined" targetRef="parallelgateway1Join" /><bpmn:sequenceFlow id="Flow_11" sourceRef="activity3_modified" targetRef="parallelgateway1" /><bpmn:sequenceFlow id="Flow_12" sourceRef="exclusivegateway2" targetRef="activity6_modified" name="Nutritional concerns, hydration deficits, dysphagia, or other comorbidities present"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Nutritional concerns, hydration deficits, dysphagia, or other comorbidities present</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_13" sourceRef="activity6_modified" targetRef="exclusivegateway2Join" /><bpmn:sequenceFlow id="Flow_14" sourceRef="exclusivegateway2" targetRef="activity7_modified" name="No nutritional concerns"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">No nutritional concerns</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_15" sourceRef="activity7_modified" targetRef="exclusivegateway2Join" /><bpmn:sequenceFlow id="Flow_16" sourceRef="parallelgateway1Join" targetRef="exclusivegateway2" /><bpmn:sequenceFlow id="Flow_17" sourceRef="exclusivegateway2Join" targetRef="activity9_modified_branch1" /><bpmn:sequenceFlow id="Flow_19" sourceRef="dummy_Flow_18" targetRef="activity8_modified" /><bpmn:sequenceFlow id="Flow_20" sourceRef="activity8_modified" targetRef="activity9_modified_branch2" /><bpmn:sequenceFlow id="Flow_21" sourceRef="exclusivegateway1" targetRef="dummy_Flow_4" name="Meets criteria for acute inpatient admission (per recommendation 2.4.2)"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Meets criteria for acute inpatient admission (per recommendation 2.4.2)</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_22" sourceRef="activity9_modified_branch1" targetRef="exclusivegateway1Join" /><bpmn:sequenceFlow id="Flow_23" sourceRef="exclusivegateway1" targetRef="dummy_Flow_18" name="Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_24" sourceRef="activity9_modified_branch2" targetRef="exclusivegateway1Join" /><bpmn:sequenceFlow id="Flow_25" sourceRef="activity1" targetRef="exclusivegateway1" /><bpmn:sequenceFlow id="Flow_26" sourceRef="exclusivegateway1Join" targetRef="EndEvent_1" /></bpmn:process></bpmn:definitions>