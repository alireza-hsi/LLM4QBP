<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn"><bpmn:process id="Process_1" isExecutable="true" name="Stroke Process"><bpmn:startEvent id="StartEvent_1" name="Start"><bpmn:outgoing>Flow_1</bpmn:outgoing></bpmn:startEvent><bpmn:endEvent id="EndEvent_1" name="End"><bpmn:incoming>Flow_33</bpmn:incoming></bpmn:endEvent><bpmn:task id="activity13" name="Ensure interprofessional stroke team uses standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: hospital administration; Objects: assessment tools, EHR system)"><bpmn:incoming>Flow_1</bpmn:incoming><bpmn:outgoing>Flow_2</bpmn:outgoing></bpmn:task><bpmn:task id="activity14" name="Ensure adequate staffing 7 days per week for stroke/TIA care (Role: hospital administration; Objects: staffing schedule)"><bpmn:incoming>Flow_2</bpmn:incoming><bpmn:outgoing>Flow_3</bpmn:outgoing></bpmn:task><bpmn:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: screening tools, patient record)"><bpmn:incoming>Flow_3</bpmn:incoming><bpmn:outgoing>Flow_32</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"><bpmn:outgoing>Flow_28</bpmn:outgoing><bpmn:outgoing>Flow_30</bpmn:outgoing><bpmn:incoming>Flow_32</bpmn:incoming></bpmn:exclusiveGateway><bpmn:task id="dummy_Flow_4" name="(dummy branch entry)"><bpmn:outgoing>Flow_5</bpmn:outgoing><bpmn:incoming>Flow_28</bpmn:incoming></bpmn:task><bpmn:task id="activity2-merged" name="Ensure stroke unit has dedicated interprofessional team (medicine, nursing, OT, PT, SLP, social work, dietitian), team members spend majority of time treating stroke patients and complete regular stroke education, and interprofessional stroke team available 7 days/week in specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: hospital administration; Objects: staffing roster, training records, staff schedules, stroke unit beds)"><bpmn:incoming>Flow_5</bpmn:incoming><bpmn:outgoing>Flow_6</bpmn:outgoing></bpmn:task><bpmn:task id="activity3" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: admission order, stroke unit beds)"><bpmn:incoming>Flow_6</bpmn:incoming><bpmn:outgoing>Flow_17</bpmn:outgoing></bpmn:task><bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway"><bpmn:outgoing>Flow_7</bpmn:outgoing><bpmn:outgoing>Flow_8</bpmn:outgoing><bpmn:outgoing>Flow_9</bpmn:outgoing><bpmn:outgoing>Flow_10</bpmn:outgoing><bpmn:outgoing>Flow_11</bpmn:outgoing><bpmn:incoming>Flow_17</bpmn:incoming></bpmn:parallelGateway><bpmn:task id="activity5" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: mobilization protocol, clinical judgement)"><bpmn:incoming>Flow_7</bpmn:incoming><bpmn:outgoing>Flow_12</bpmn:outgoing></bpmn:task><bpmn:task id="activity6" name="Complete AlphaFIM on or by day 3 after admission (admission day is day 1) (Role: clinical team; Objects: AlphaFIM tool, patient record)"><bpmn:incoming>Flow_8</bpmn:incoming><bpmn:outgoing>Flow_13</bpmn:outgoing></bpmn:task><bpmn:task id="activity7" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinical team; Objects: risk assessment tool)"><bpmn:incoming>Flow_9</bpmn:incoming><bpmn:outgoing>Flow_14</bpmn:outgoing></bpmn:task><bpmn:task id="activity8" name="Encourage early mobilization and adequate hydration to help prevent venous thromboembolism (Role: clinical team; Objects: mobilization protocol, hydration plan)"><bpmn:incoming>Flow_10</bpmn:incoming><bpmn:outgoing>Flow_15</bpmn:outgoing></bpmn:task><bpmn:task id="activi