      <ns0:outgoing>Flow_1</ns0:outgoing>
    </ns0:startEvent>
    <ns0:endEvent id="EndEvent_1" name="End">
      <ns0:incoming>Flow_27</ns0:incoming>
    </ns0:endEvent>
    <ns0:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: screening tools, patient records)">
      <ns0:incoming>Flow_1</ns0:incoming>
      <ns0:outgoing>Flow_26</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <ns0:incoming>Flow_26</ns0:incoming>
      <ns0:outgoing>Flow_22</ns0:outgoing>
      <ns0:outgoing>Flow_24</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity3" name="Ensure stroke unit has dedicated interprofessional team (nursing, physiotherapy, occupational therapy, speech-language pathology, medicine, social work, dietitian) (Role: hospital administrator; Objects: staffing roster)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4" name="Ensure interprofessional stroke team available 7 days/week (Role: hospital administrator; Objects: staffing schedule)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: admission order, bed assignment)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5" name="Maintain stroke expertise through majority stroke caseload and regular stroke education (Role: stroke unit team; Objects: training records, caseload data)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_7</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity6" name="Assess patient within 48 hours of admission and formulate management plan (Role: physiotherapist, occupational therapist, speech-language pathologist; Objects: assessment tools, assessment results, patient chart)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_8</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7" name="Select appropriate standardized, valid assessment tools (Role: clinical team; Objects: assessment tool list)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_9</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Evaluate stroke-related impairments and functional status using selected tools (Role: clinical team; Objects: assessment tools, patient chart)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity9" name="Complete AlphaFIM on or by day 3 after admission (admission day is day 1) (Role: clinical team; Objects: AlphaFIM tool, patient chart)">
      <ns0:incoming>Flow_10</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity10" name="Assess all stroke patients for risk of developing venous thromboembolism (Role: clinical team; Objects: risk assessment tool)">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity11" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: mobilization protocol, clinical judgement)">
      <ns0:incoming>Flow_12</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity12" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: mobilization protocol, hydration plan)">
      <ns0:incoming>Flow_13</ns0:incoming>
      <ns0:outgoing>Flow_14</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity13_new" name="Assess nutrition and hydration status within first 48 hours of admission using valid screening tools (Role: clinical team; Objects: nutrition screening tool, hydration screening tool)">
      <ns0:incoming>Flow_14</ns0:incoming>
      <ns0:outgoing>Flow_19</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway">
      <ns0:incoming>Flow_19</ns0:incoming>
      <ns0:outgoing>Flow_15</ns0:outgoing>
      <ns0:outgoing>Flow_17</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity15" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements (Role: clinical team; Objects: referral form, patient chart)">
      <ns0:incoming>Flow_15</ns0:incoming>
      <ns0:outgoing>Flow_16</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity16" name="Continue standard nutrition and hydration monitoring (Role: clinical team; Objects: patient chart)">
      <ns0:incoming>Flow_17</ns0:incoming>
      <ns0:outgoing>Flow_18</ns0:outgoing>
    </ns0:task>
