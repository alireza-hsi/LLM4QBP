      <ns0:incoming>Flow_16</ns0:incoming>
      <ns0:incoming>Flow_18</ns0:incoming>
      <ns0:outgoing>Flow_20</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity17" name="Initiate cross-continuum secondary prevention assessments and therapies (Modules 5 and 10), regardless of discharge or in-hospital rehabilitation plan (Role: clinical team; Objects: prevention protocols, patient chart)">
      <ns0:incoming>Flow_20</ns0:incoming>
      <ns0:outgoing>Flow_21</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity18" name="Ensure interprofessional stroke team uses standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: hospital administrator; Objects: EHR system, assessment tools)">
      <ns0:incoming>Flow_21</ns0:incoming>
      <ns0:outgoing>Flow_23</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity19" name="Follow alternative pathway; recommendations in Module 3a not applicable (Role: clinical team; Objects: clinical guidelines)">
      <ns0:incoming>Flow_24</ns0:incoming>
      <ns0:outgoing>Flow_25</ns0:outgoing>
    </ns0:task>
