      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_25</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="activity1.1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression and forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools, TIA clinic)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_24</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <bpmn:incoming>Flow_24</bpmn:incoming>
      <bpmn:outgoing>Flow_20</bpmn:outgoing>
      <bpmn:outgoing>Flow_22</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="dummy_Flow_2" name="(dummy branch entry)">
      <bpmn:incoming>Flow_20</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting clinician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Ensure dedicated interprofessional stroke team with expertise in stroke care (medicine, nursing, OT, PT, SLP, social work, dietitian) (Role: hospital administration; Objects: staffing schedule)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Ensure interprofessional stroke team available 7 days a week (Role: hospital administration; Objects: staffing schedule)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5" name="Ensure use of standardized, validated assessment tools (preferably electronic, linked to EHR) (Role: hospital administration; Objects: assessment tools, EHR)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6_merged" name="Within 48 hours of admission: assess stroke patient using standardized, valid assessment tools (including AlphaFIM by day 3), assess for risk of venous thromboembolism, assess nutrition and hydration status, and formulate management plan (Role: PT, OT, SLP, clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment findings)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway_exp2" name="Exclusive Gateway">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="activity7.1" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support (Role: clinical team; Objects: patient, referral form)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity7.2" name="Continue standard monitoring (Role: clinical team; Objects: patient)">
