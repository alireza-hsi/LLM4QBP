    <ns0:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: patient, stroke unit)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
