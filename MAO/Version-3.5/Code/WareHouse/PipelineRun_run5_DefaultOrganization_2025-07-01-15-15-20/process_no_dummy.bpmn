      <ns0:outgoing>Flow_19</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity10" name="Encourage early mobilization and adequate hydration for all acute stroke patients to help prevent venous thromboembolism (Role: stroke unit team; Objects: mobilization protocol, hydration plan)">
      <ns0:incoming>Flow_16</ns0:incoming>
      <ns0:outgoing>Flow_20</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity11" name="Ensure all assessments and documentation use standardized, validated tools (preferably electronic and linked to EHR) (Role: stroke unit team; Objects: EHR, assessment tools)">
