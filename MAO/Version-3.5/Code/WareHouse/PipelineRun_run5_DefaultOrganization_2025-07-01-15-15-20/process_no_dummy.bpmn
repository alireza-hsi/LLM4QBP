      <ns0:outgoing>Flow_16</ns0:outgoing>
      <ns0:outgoing>Flow_17</ns0:outgoing>
    </ns0:parallelGateway>
    <ns0:task id="activity6" name="Within 48 hours of admission, use standardized, valid assessment tools to evaluate stroke-related impairments and functional status, complete AlphaFIM (on or by day 3), assess risk of venous thromboembolism, assess nutrition and hydration status, then formulate management plan based on assessment results (Role: physiotherapist, occupational therapist, speech-language pathologist, clinician; Objects: assessment tools, AlphaFIM tool, risk assessment tool, nutrition screening tool, hydration screening tool, patient record)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway">
