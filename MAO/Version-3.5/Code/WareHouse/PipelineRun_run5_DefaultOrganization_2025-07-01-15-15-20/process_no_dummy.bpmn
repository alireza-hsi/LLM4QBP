      <ns0:outgoing>Flow_24</ns0:outgoing>
      <ns0:outgoing>Flow_26</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: admission order, stroke unit beds)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Ensure stroke unit team includes medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) (Role: hospital administration; Objects: staffing roster)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4" name="Ensure interprofessional stroke team available 7 days/week (Role: hospital administration; Objects: staffing schedule)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity5" name="Maintain stroke expertise through regular education and majority time spent treating stroke patients (Role: stroke unit team; Objects: training records, staff schedules)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_22</ns0:outgoing>
    </ns0:task>
