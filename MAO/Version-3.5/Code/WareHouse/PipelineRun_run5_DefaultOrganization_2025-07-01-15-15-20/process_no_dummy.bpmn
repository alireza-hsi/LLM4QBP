      <ns0:outgoing>Flow_9</ns0:outgoing>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity7" name="Refer to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet nutritional/fluid requirements (Role: clinician; Objects: referral form, patient record)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Continue standard nutrition and hydration monitoring (Role: clinician; Objects: monitoring tools)">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
