      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2-1" name="Assign patient to co-located stroke unit bed (75% stroke occupancy) (Role: admitting clinician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2-2" name="Initiate admission procedures for stroke unit (Role: admitting clinician; Objects: patient, admission forms)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Ensure dedicated interprofessional stroke team with expertise in stroke care (minimum: nursing, physiotherapy, occupational therapy, speech-language pathology, medicine, social work, dietitian) (Role: hospital administration; Objects: staffing plan)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Ensure stroke unit team members spend majority of time treating stroke patients and regularly complete stroke education (Role: hospital administration; Objects: staff education records)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5" name="Ensure access to specialized interprofessional team 7 days a week (Role: hospital administration; Objects: staffing schedule)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6-merged" name="Conduct comprehensive assessments (physiotherapy, speech-language pathology, occupational therapy within 48 hours, AlphaFIM by day 3, venous thromboembolism risk, nutrition/hydration status within 48 hours) and formulate management plan based on assessment results (Role: clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results)">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity7" name="Mobilize patient early (between 24-48 hours of stroke onset), unless contraindicated; avoid frequent out-of-bed activity within first 24 hours, as per management plan (Role: clinical team; Objects: patient, mobilization protocol, management plan)">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity11" name="Encourage early mobilization and adequate hydration for all acute stroke patients to help prevent venous thromboembolism, as per management plan (Role: clinical team; Objects: patient, hydration protocol, management plan)">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity13" name="If nutritional concerns, hydration deficits, dysphagia, or other comorbidities present per management plan, refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements; otherwise, continue standard monitoring of nutrition and hydration (Role: clinical team; Objects: patient, referral form, management plan)">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1Join" name="Parallel Merge">
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity15" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (per Modules 5 and 10), regardless of discharge destination (Role: clinical team; Objects: patient, prevention protocol)">
      <bpmn:incoming>Flow_17</bpmn:incoming>
      <bpmn:outgoing>Flow_19</bpmn:outgoing>
    </bpmn:task>
