      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity1-2" name="Forward screening/assessment results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening results)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_22</bpmn:outgoing>
    </bpmn:task>
