      <bpmn:incoming>Flow_21</bpmn:incoming>
      <bpmn:outgoing>Flow_23</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="activity17" name="Ensure interprofessional stroke team uses standardized, validated assessment tools (preferably electronic and EHR-linked) (Role: hospital administration; Objects: assessment tools, EHR)">
      <bpmn:incoming>Flow_23</bpmn:incoming>
      <bpmn:outgoing>Flow_24</bpmn:outgoing>
    </bpmn:task>
