      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting clinician; Objects: patient, stroke unit beds)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Ensure stroke unit team includes medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and dietitian (Role: hospital administration; Objects: staffing plan)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Ensure stroke unit team has stroke expertise and regular education (Role: hospital administration; Objects: training records)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5" name="Ensure interprofessional stroke team is available 7 days a week (Role: hospital administration; Objects: staffing schedule)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity8" name="Ensure use of standardized, validated assessment tools in electronic format linked to EHR where possible (Role: hospital administration; Objects: assessment tools, EHR)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity9" name="Ensure adequate staffing 7 days per week (Role: hospital administration; Objects: staffing plan)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity7_merged" name="Perform all required assessments (impairments, functional status, AlphaFIM, VTE risk, nutrition/hydration) and formulate comprehensive management plan for stroke patient (Role: clinical team (physiotherapist, occupational therapist, speech-language pathologist, others as needed); Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results)">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
