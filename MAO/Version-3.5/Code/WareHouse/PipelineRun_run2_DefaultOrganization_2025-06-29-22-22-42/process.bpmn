    </bpmn:task>
    <bpmn:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting physician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Ensure core stroke unit team consists of medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) (Role: hospital administration; Objects: staffing plan)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4" name="Ensure stroke unit team members spend majority of time treating stroke patients and regularly complete stroke care education (Role: hospital administration; Objects: staffing records, education logs)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5" name="Ensure access to specialized interprofessional stroke team 7 days a week (Role: hospital administration; Objects: staffing schedule)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6_merged" name="Perform comprehensive assessment within 48 hours of admission using standardized, valid tools (including AlphaFIM, risk of venous thromboembolism, nutrition and hydration status); evaluate stroke-related impairments and functional status; formulate management plan based on all assessment results (including mobilization, hydration, and nutrition status) (Role: clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results, management plan)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
