<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn"><bpmn:process id="Process_1" isExecutable="true" name="Stroke Process"><bpmn:startEvent id="StartEvent_1" name="Start"><bpmn:outgoing>Flow_1</bpmn:outgoing></bpmn:startEvent><bpmn:endEvent id="EndEvent_1" name="End"><bpmn:incoming>Flow_30</bpmn:incoming></bpmn:endEvent><bpmn:task id="activity1-1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation (Role: ED clinician; Objects: patient, screening tools)"><bpmn:incoming>Flow_1</bpmn:incoming><bpmn:outgoing>Flow_2</bpmn:outgoing></bpmn:task><bpmn:task id="activity1-2" name="Forward results to TIA clinic (Role: ED clinician; Objects: patient, screening results)"><bpmn:incoming>Flow_2</bpmn:incoming><bpmn:outgoing>Flow_27</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"><bpmn:outgoing>Flow_23</bpmn:outgoing><bpmn:outgoing>Flow_25</bpmn:outgoing><bpmn:incoming>Flow_27</bpmn:incoming></bpmn:exclusiveGateway><bpmn:task id="dummy_Flow_3" name="(dummy branch entry)"><bpmn:outgoing>Flow_4</bpmn:outgoing><bpmn:incoming>Flow_23</bpmn:incoming></bpmn:task><bpmn:task id="activity2" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting clinician; Objects: patient, stroke unit)"><bpmn:incoming>Flow_4</bpmn:incoming><bpmn:outgoing>Flow_5</bpmn:outgoing></bpmn:task><bpmn:task id="activity3" name="Ensure interprofessional stroke team with expertise in medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) (Role: hospital administration; Objects: staffing, stroke unit)"><bpmn:incoming>Flow_5</bpmn:incoming><bpmn:outgoing>Flow_6</bpmn:outgoing></bpmn:task><bpmn:task id="activity4" name="Ensure stroke unit team members spend majority of time treating stroke patients and complete regular stroke care education (Role: hospital administration; Objects: staffing, education records)"><bpmn:incoming>Flow_6</bpmn:incoming><bpmn:outgoing>Flow_7</bpmn:outgoing></bpmn:task><bpmn:task id="activity5" name="Ensure interprofessional stroke team available 7 days a week (Role: hospital administration; Objects: staffing schedules)"><bpmn:incoming>Flow_7</bpmn:incoming><bpmn:outgoing>Flow_8</bpmn:outgoing></bpmn:task><bpmn:task id="activity6" name="Within 48 hours of admission, perform comprehensive assessment (including evaluation of stroke-related impairments and functional status, AlphaFIM assessment by day 3, risk assessment for venous thromboembolism, and assessment of nutrition and hydration status using valid screening tool; if nutritional concerns, hydration deficits, dysphagia, or other comorbidities are present, refer to dietitian within 7 days for recommendations and consideration of enteral nutrition support) and formulate management plan based on assessment results (including dietitian recommendations if referred) (Role: physiotherapist, speech-language pathologist, occupational therapist, clinical team; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results, referral form, management plan)"><bpmn:incoming>Flow_8</bpmn:incoming><bpmn:outgoing>Flow_18</bpmn:outgoing></bpmn:task><bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway"><bpmn:outgoing>Flow_12</bpmn:outgoing><bpmn:outgoing>Flow_13</bpmn:outgoing><bpmn:outgoing>Flow_14</bpmn:outgoing><bpmn:incoming>Flow_18</bpmn:incoming></bpmn:parallelGateway><bpmn:task id="activity10" name="Encourage early mobilization (between 24-48 hours of stroke onset, unless contraindicated) according to management plan (Role: clinical team; Objects: patient, mobilization plan)"><bpmn:incoming>Flow_12</bpmn:incoming><bpmn:outgoing>Flow_15</bpmn:outgoing></bpmn:task><bpmn:task id="activity11" name="Encourage adequate hydration for all acute stroke patients to help prevent venous thromboembolism according to management plan (Role: clinical team; Objects: patient, hydration plan)"><bpmn:incoming>Flow_13</bpmn:incoming><bpmn:outgoing>Flow_16</bpmn:outgoing></bpmn:task><bpmn:task id="dummy_Flow_9" name="(dummy branch entry)"><bpmn:outgoing>Flow_10</bpmn:outgoing><bpmn:incoming>Flow_14</bpmn:incoming></bpmn:task><bpmn:task id="activity15-1" name="Conduct secondary prevention assessment (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_10</bpmn:incoming><bpmn:outgoing>Flow_11</bpmn:outgoing></bpmn:task><bpmn:task id="activity15-2" name="Initiate secondary prevention therapies (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_11</bpmn:incoming><bpmn:outgoing>Flow_17</bpmn:outgoing></bpmn:task><bpmn:parallelGateway id="parallelgateway1Join" name="Parallel Merge"><bpmn:incoming>Flow_15</bpmn:incoming><bpmn:incoming>Flow_16</bpmn:incoming><bpmn:incoming>Flow_17</bpmn:incoming><bpmn:outgoing>Flow_24</bpmn:outgoing></bpmn:parallelGateway><bpmn:task id="dummy_Flow_19" name="(dummy branch entry)"><bpmn:outgoing>Flow_20</bpmn:outgoing><bpmn:incoming>Flow_25</bpmn:incoming></bpmn:task><bpmn:task id="activity16" name="Do not apply acute inpatient stroke unit recommendations; proceed with appropriate outpatient/TIA clinic management (Role: clinical team; Objects: patient)"><bpmn:incoming>Flow_20</bpmn:incoming><bpmn:outgoing>Flow_21</bpmn:outgoing></bpmn:task><bpmn:task id="activity17-1" name="Conduct secondary prevention assessment (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_21</bpmn:incoming><bpmn:outgoing>Flow_22</bpmn:outgoing></bpmn:task><bpmn:task id="activity17-2" name="Initiate secondary prevention therapies (Role: clinical team; Objects: patient, prevention protocols)"><bpmn:incoming>Flow_22</bpmn:incoming><bpmn:outgoing>Flow_26</bpmn:outgoing></bpmn:task><bpmn:exclusiveGateway id="exclusivegateway1Join" name="Exclusive Merge"><bpmn:incoming>Flow_24</bpmn:incoming><bpmn:incoming>Flow_26</bpmn:incoming><bpmn:outgoing>Flow_28</bpmn:outgoing></bpmn:exclusiveGateway><bpmn:task id="activity18" name="Ensure interprofessional stroke team uses standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: hospital administration; Objects: assessment tools, EHR)"><bpmn:incoming>Flow_28</bpmn:incoming><bpmn:outgoing>Flow_29</bpmn:outgoing></bpmn:task><bpmn:task id="activity19" name="Ensure adequate staffing 7 days per week for stroke/TIA care (Role: hospital administration; Objects: staffing schedules)"><bpmn:incoming>Flow_29</bpmn:incoming><bpmn:outgoing>Flow_30</bpmn:outgoing></bpmn:task><bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="activity1-1" /><bpmn:sequenceFlow id="Flow_2" sourceRef="activity1-1" targetRef="activity1-2" /><bpmn:sequenceFlow id="Flow_4" sourceRef="dummy_Flow_3" targetRef="activity2" /><bpmn:sequenceFlow id="Flow_5" sourceRef="activity2" targetRef="activity3" /><bpmn:sequenceFlow id="Flow_6" sourceRef="activity3" targetRef="activity4" /><bpmn:sequenceFlow id="Flow_7" sourceRef="activity4" targetRef="activity5" /><bpmn:sequenceFlow id="Flow_8" sourceRef="activity5" targetRef="activity6" /><bpmn:sequenceFlow id="Flow_10" sourceRef="dummy_Flow_9" targetRef="activity15-1" /><bpmn:sequenceFlow id="Flow_11" sourceRef="activity15-1" targetRef="activity15-2" /><bpmn:sequenceFlow id="Flow_12" sourceRef="parallelgateway1" targetRef="activity10" /><bpmn:sequenceFlow id="Flow_13" sourceRef="parallelgateway1" targetRef="activity11" /><bpmn:sequenceFlow id="Flow_14" sourceRef="parallelgateway1" targetRef="dummy_Flow_9" /><bpmn:sequenceFlow id="Flow_15" sourceRef="activity10" targetRef="parallelgateway1Join" /><bpmn:sequenceFlow id="Flow_16" sourceRef="activity11" targetRef="parallelgateway1Join" /><bpmn:sequenceFlow id="Flow_17" sourceRef="activity15-2" targetRef="parallelgateway1Join" /><bpmn:sequenceFlow id="Flow_18" sourceRef="activity6" targetRef="parallelgateway1" /><bpmn:sequenceFlow id="Flow_20" sourceRef="dummy_Flow_19" targetRef="activity16" /><bpmn:sequenceFlow id="Flow_21" sourceRef="activity16" targetRef="activity17-1" /><bpmn:sequenceFlow id="Flow_22" sourceRef="activity17-1" targetRef="activity17-2" /><bpmn:sequenceFlow id="Flow_23" sourceRef="exclusivegateway1" targetRef="dummy_Flow_3" name="Meets criteria for acute inpatient admission (per recommendation 2.4.2)"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Meets criteria for acute inpatient admission (per recommendation 2.4.2)</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_24" sourceRef="parallelgateway1Join" targetRef="exclusivegateway1Join" /><bpmn:sequenceFlow id="Flow_25" sourceRef="exclusivegateway1" targetRef="dummy_Flow_19" name="Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_26" sourceRef="activity17-2" targetRef="exclusivegateway1Join" /><bpmn:sequenceFlow id="Flow_27" sourceRef="activity1-2" targetRef="exclusivegateway1" /><bpmn:sequenceFlow id="Flow_28" sourceRef="exclusivegateway1Join" targetRef="activity18" /><bpmn:sequenceFlow id="Flow_29" sourceRef="activity18" targetRef="activity19" /><bpmn:sequenceFlow id="Flow_30" sourceRef="activity19" targetRef="EndEvent_1" /></bpmn:process></bpmn:definitions>