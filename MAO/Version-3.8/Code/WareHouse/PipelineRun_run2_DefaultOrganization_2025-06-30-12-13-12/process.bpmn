    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_17" sourceRef="activity8" targetRef="exclusivegateway1Join" />
    <bpmn:sequenceFlow id="Flow_18" name="Does not meet criteria for acute inpatient admission (e.g., TIA or minor/nondisabling stroke)" sourceRef="exclusivegateway1" targetRef="activity9">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Does not meet criteria for acute inpatient admission (e.g., TIA or minor/nondisabling stroke)</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
