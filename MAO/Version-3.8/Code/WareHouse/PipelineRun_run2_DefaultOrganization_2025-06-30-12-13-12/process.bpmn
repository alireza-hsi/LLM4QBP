      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Nutritional concerns, hydration deficits, dysphagia, or comorbidities present</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="activity6" targetRef="exclusivegateway2Join" />
    <bpmn:sequenceFlow id="Flow_8" name="No nutritional concerns, hydration deficits, dysphagia, or comorbidities" sourceRef="exclusivegateway2" targetRef="activity7">
