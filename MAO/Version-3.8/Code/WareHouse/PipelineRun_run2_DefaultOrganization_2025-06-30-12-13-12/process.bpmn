  <bpmn:process id="Process_1" name="Stroke Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_21</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation and forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_20</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <bpmn:incoming>Flow_20</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="dummy_Flow_2" name="(dummy branch entry)">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Ensure dedicated interprofessional stroke team with expertise in stroke care (nursing, physiotherapy, occupational therapy, speech-language pathology, medicine, social work, dietitian), ensure team availability 7 days a week, and ensure use of standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: hospital administration; Objects: staffing schedule, assessment tools, EHR)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) (Role: admitting clinician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity4_modified" name="Within 48 hours of admission, perform all required assessments (standardized assessment tools, AlphaFIM, risk of venous thromboembolism, nutrition/hydration status), and formulate a management plan based on assessment results (Role: physiotherapist, occupational therapist, speech-language pathologist, clinician; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity5" name="Encourage early mobilization (between 24-48 hours of stroke onset unless contraindicated; avoid frequent out-of-bed activity within first 24 hours unless clinically justified) and adequate hydration to help prevent venous thromboembolism (Role: clinical team; Objects: patient)">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="activity6" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet nutritional/fluid requirements (Role: clinician; Objects: patient, referral)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity7" name="Continue standard monitoring (Role: clinician; Objects: patient)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway2Join" name="Exclusive Merge">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:parallelGateway id="parallelgateway1Join" name="Parallel Merge">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity8" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (Modules 5 and 10) to all patients, whether admitted to in-hospital rehabilitation or discharged (Role: clinical team; Objects: patient, prevention protocols)">
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity9" name="Do not apply acute inpatient stroke unit recommendations; proceed with alternative care pathway (Role: clinician; Objects: patient)">
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_19</bpmn:outgoing>
    </bpmn:task>
