  <bpmn:process id="Process_1" name="Stroke Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_20</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="activity1-merged" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation and forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, assessment tools, assessment results)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2" name="Determine eligibility for acute inpatient admission per recommendation 2.4.2 (Role: admitting clinician; Objects: patient, eligibility criteria)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="dummy_Flow_3" name="(dummy branch entry)">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3-1" name="Assign patient to available stroke unit bed (Role: admitting clinician; Objects: patient, stroke unit beds)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3-2" name="Transfer patient to stroke unit (Role: admitting clinician; Objects: patient, stroke unit)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activityMerged1" name="Conduct all required assessments (stroke-related impairments, functional status using standardized tools, AlphaFIM by day 3, venous thromboembolism risk, nutrition and hydration status within 48 hours) and formulate management plan based on assessment results (Role: clinical team (including physiotherapist, occupational therapist, speech-language pathologist, dietitian); Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity8" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: patient, mobilization protocol)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity12" name="Encourage early mobilization and adequate hydration to prevent venous thromboembolism (Role: clinical team; Objects: patient, hydration protocol)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway">
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="activity14" name="Refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements (Role: clinical team; Objects: patient, referral form)">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity15" name="Continue routine monitoring of nutrition and hydration (Role: clinical team; Objects: patient)">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>
