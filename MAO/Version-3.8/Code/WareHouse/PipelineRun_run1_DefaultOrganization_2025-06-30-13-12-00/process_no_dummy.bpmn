  <ns0:process id="Process_1" name="Stroke Process" isExecutable="true">
    <ns0:startEvent id="StartEvent_1" name="Start">
      <ns0:outgoing>Flow_1</ns0:outgoing>
    </ns0:startEvent>
    <ns0:endEvent id="EndEvent_1" name="End">
      <ns0:incoming>Flow_23</ns0:incoming>
    </ns0:endEvent>
    <ns0:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools)">
      <ns0:incoming>Flow_1</ns0:incoming>
      <ns0:outgoing>Flow_22</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <ns0:incoming>Flow_22</ns0:incoming>
      <ns0:outgoing>Flow_18</ns0:outgoing>
      <ns0:outgoing>Flow_20</ns0:outgoing>
    </ns0:exclusiveGateway>
    <ns0:task id="activity2" name="Set up specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy); ensure core team (medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, clinical nutrition) spends majority of time treating stroke patients, completes regular stroke education, and is available 7 days a week (Role: hospital administration; Objects: stroke unit, staffing plan, staffing records, education logs, staffing schedule)">
      <ns0:incoming>Flow_3</ns0:incoming>
      <ns0:outgoing>Flow_4</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity3" name="Admit patient to specialized stroke unit (Role: admitting physician; Objects: patient, stroke unit)">
      <ns0:incoming>Flow_4</ns0:incoming>
      <ns0:outgoing>Flow_5</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity4_merged" name="Within 48 hours of admission, select and use standardized, valid assessment tools to evaluate stroke-related impairments and functional status, complete AlphaFIM assessment, assess risk of venous thromboembolism, assess nutrition and hydration status, and formulate management plan (Role: physiotherapist, speech-language pathologist, occupational therapist; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results, management plan)">
      <ns0:incoming>Flow_5</ns0:incoming>
      <ns0:outgoing>Flow_16</ns0:outgoing>
    </ns0:task>
    <ns0:parallelGateway id="parallelgateway1" name="Parallel Gateway">
      <ns0:incoming>Flow_16</ns0:incoming>
      <ns0:outgoing>Flow_6</ns0:outgoing>
      <ns0:outgoing>Flow_7</ns0:outgoing>
      <ns0:outgoing>Flow_8</ns0:outgoing>
      <ns0:outgoing>Flow_9</ns0:outgoing>
      <ns0:outgoing>Flow_10</ns0:outgoing>
    </ns0:parallelGateway>
    <ns0:task id="activity5" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours (Role: clinical team; Objects: patient, mobilization aids)">
      <ns0:incoming>Flow_6</ns0:incoming>
      <ns0:outgoing>Flow_11</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity6" name="Encourage early mobilization and adequate hydration for all acute stroke patients to help prevent venous thromboembolism (Role: clinical team; Objects: patient, hydration plan)">
      <ns0:incoming>Flow_7</ns0:incoming>
      <ns0:outgoing>Flow_12</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity7" name="If nutrition and hydration assessment indicates concerns, refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet requirements; otherwise, continue standard nutrition and hydration monitoring (Role: clinical team; Objects: patient, referral form)">
      <ns0:incoming>Flow_8</ns0:incoming>
      <ns0:outgoing>Flow_13</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity8" name="Ensure use of standardized, validated assessment tools, preferably in electronic format linked to EHR (Role: clinical team; Objects: assessment tools, EHR)">
      <ns0:incoming>Flow_9</ns0:incoming>
      <ns0:outgoing>Flow_14</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity9" name="Ensure adequate staffing 7 days per week (Role: clinical team; Objects: staffing schedule)">
      <ns0:incoming>Flow_10</ns0:incoming>
      <ns0:outgoing>Flow_15</ns0:outgoing>
    </ns0:task>
    <ns0:parallelGateway id="parallelgateway1Join" name="Parallel Merge">
      <ns0:incoming>Flow_11</ns0:incoming>
      <ns0:incoming>Flow_12</ns0:incoming>
      <ns0:incoming>Flow_13</ns0:incoming>
      <ns0:incoming>Flow_14</ns0:incoming>
      <ns0:incoming>Flow_15</ns0:incoming>
      <ns0:outgoing>Flow_17</ns0:outgoing>
    </ns0:parallelGateway>
    <ns0:task id="activity10" name="Provide appropriate cross-continuum secondary prevention assessments and therapies (Modules 5 and 10) to all patients, whether admitted to in-hospital rehabilitation or discharged (Role: clinical team; Objects: patient, prevention protocols)">
      <ns0:incoming>Flow_17</ns0:incoming>
      <ns0:outgoing>Flow_19</ns0:outgoing>
    </ns0:task>
    <ns0:task id="activity11" name="Do not apply Module 3a recommendations; proceed with alternative care pathway (Role: clinical team; Objects: patient)">
      <ns0:incoming>Flow_20</ns0:incoming>
      <ns0:outgoing>Flow_21</ns0:outgoing>
    </ns0:task>
    <ns0:exclusiveGateway id="exclusivegateway1Join" name="Exclusive Merge">
      <ns0:incoming>Flow_19</ns0:incoming>
      <ns0:incoming>Flow_21</ns0:incoming>
      <ns0:outgoing>Flow_23</ns0:outgoing>
    </ns0:exclusiveGateway>
