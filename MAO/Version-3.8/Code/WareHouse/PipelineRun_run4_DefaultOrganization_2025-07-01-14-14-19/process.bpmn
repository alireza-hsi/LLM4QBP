    <bpmn:sequenceFlow id="Flow_14" sourceRef="parallelgateway1Join" targetRef="activity8_new" />
    <bpmn:sequenceFlow id="Flow_15" name="Meets criteria for acute inpatient admission (per recommendation 2.4.2)" sourceRef="exclusivegateway1" targetRef="dummy_Flow_2">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Meets criteria for acute inpatient admission (per recommendation 2.4.2)</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_16" sourceRef="activity8_new" targetRef="exclusivegateway1Join" />
    <bpmn:sequenceFlow id="Flow_17" name="Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)" sourceRef="exclusivegateway1" targetRef="activity9_new">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Does not meet criteria for acute inpatient admission (e.g., TIA or minor nondisabling stroke)</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_18" sourceRef="activity9_new" targetRef="exclusivegateway1Join" />
    <bpmn:sequenceFlow id="Flow_19" sourceRef="activity1" targetRef="exclusivegateway1" />
    <bpmn:sequenceFlow id="Flow_20" sourceRef="exclusivegateway1Join" targetRef="EndEvent_1" />
