  <bpmn:process id="Process_1" name="Stroke Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_20</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="activity1" name="Screen/assess for diabetes, dental issues, cognitive impairment, and depression upon ED presentation; forward results to TIA clinic for follow-up (Role: ED clinician; Objects: patient, screening tools)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_19</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway">
      <bpmn:incoming>Flow_19</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="dummy_Flow_2" name="(dummy branch entry)">
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity2_new" name="Admit patient to specialized, geographically defined stroke unit (co-located beds, 75% stroke occupancy) and ensure interprofessional stroke team with expertise in medicine, nursing, occupational therapy, physiotherapy, speech-language pathology, social work, and clinical nutrition (dietitian) is available 7 days a week, with team members spending majority of time treating stroke patients and regularly completing stroke care education (Role: hospital administration; Objects: patient, stroke unit, staffing plan, staffing records, education records, staffing schedule)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity3_merged" name="Within 48 hours of admission, perform all relevant assessments (using standardized, valid tools, AlphaFIM, risk of venous thromboembolism, nutrition/hydration status) and formulate a comprehensive management plan based on assessment results (Role: physiotherapist, speech-language pathologist, occupational therapist, clinicians; Objects: patient, assessment tools, AlphaFIM tool, risk assessment tool, nutrition/hydration screening tool, assessment results, management plan)">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1" name="Parallel Gateway">
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity4_new" name="Mobilize patient early (between 24-48 hours of stroke onset) unless contraindicated; avoid frequent out-of-bed activity within first 24 hours unless clinically justified (Role: clinical team; Objects: patient, mobilization plan)">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity5_new" name="Encourage early mobilization and adequate hydration for all acute stroke patients to help prevent venous thromboembolism (Role: clinical team; Objects: patient, hydration plan)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity6_new" name="If nutrition/hydration assessment identifies concerns, refer patient to dietitian within 7 days of admission for recommendations and consideration of enteral nutrition support if unable to meet nutritional/fluid requirements; otherwise, continue standard nutritional monitoring (Role: clinicians; Objects: patient, referral form)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity7_new" name="Ensure use of standardized, validated assessment tools (preferably electronic and linked to EHR) (Role: clinical team; Objects: assessment tools, EHR)">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="parallelgateway1Join" name="Parallel Merge">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="activity8_new" name="Provide cross-continuum secondary prevention assessments and therapies (Modules 5 and 10) regardless of discharge destination (Role: clinical team; Objects: patient, prevention protocols)">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="activity9_new" name="Do not apply acute inpatient stroke unit recommendations; proceed with alternative care pathway (Role: clinical team; Objects: patient)">
      <bpmn:incoming>Flow_17</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="exclusivegateway1Join" name="Exclusive Merge">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_20</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="activity1" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="dummy_Flow_2" targetRef="activity2_new" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="activity2_new" targetRef="activity3_merged" />
